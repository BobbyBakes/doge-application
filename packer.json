{
  "variables": {
    "aws_access_key": "",
    "aws_secret_key": "",
    "proxy_uname": "",
    "proxy_password": ""
  },
	"builders": [
		{
				"type": "docker",
				"image": "ubuntu:14.04",
				"commit": true
		}
	],
	"provisioners": [
		{
			"type": "shell",
			"script": "scripts/install_java.sh",
      "environment_vars": ["PROXY_UNAME={{user `proxy_uname`}}",
                           "PROXY_PASSWORD={{user `proxy_password`}}"]
		}
	]
}
